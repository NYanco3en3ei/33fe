{
  "version": 2,
  "builds": [{"src": "package.json", "use": "@vercel/static-build", "config": {"distDir": "dist"}}],
  "routes": [
    {"src": "/assets/(.*)", "dest": "/assets/$1"},
    {"src": "/(.*\\.(js|css|json|ico))$", "dest": "/$1"},
    {"src": "/(.*)", "dest": "/index.html"}
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build"
}
